{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "camelcase": "off",
        "indent": "off",
        "import/resolver": {
            "node": {
              "paths": ["src"],
              "extensions": [".js", ".jsx", ".ts", ".tsx"]
            }
        }
    }
}
